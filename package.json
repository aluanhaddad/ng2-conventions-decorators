    "@types/systemjs": "^0.19.31",
    "@types/tape": "^4.2.27",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "systemjs": "^0.19.39",
    "tape": "latest",
    "tslint": "next",
    "typescript": "next"
  },
  "repository": {
